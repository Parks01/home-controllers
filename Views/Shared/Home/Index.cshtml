@{
  Layout = "_Layout";
}
<h1>Welcome to the CdOrganizer site.</h1>

<form action="/createArtist" method="post">
  <div class="form-group">
    <label for="artist-name">Artist Name:</label>
    <input id="artist-name" name="artist-name" type="text">
  </div>
  <div class="form-group">
    <label for="cd-name">CD Name:</label>
    <input id="cd-name" name="cd-name" type="text">
  </div>
  <div class="form-group">
    <label for="genre">Genre:</label>
    <input id="genre-name" name="genre-name" type="text">
  </div>
  <div class="form-group">
    <label for="year">Year:</label>
    <input id="year" name="year" type="text">
  </div>
  <button type="submit">Creat an Artist</button>
</form>
